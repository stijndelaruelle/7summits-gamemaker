            <kind>1</kind>
            <string>///Thowing a rope
if (m_ReloadTime &gt; 0)
{
    m_ReloadTime -= global.DeltaTime;
}
else if (shootKey &amp;&amp; m_NumRopes &gt; 0)
{
    var bullet = instance_create(x, y, obj_rope_flying);

    //Determine bullet direction
    bullet.m_DirectionX = m_HorizDirection * 0.10;
    bullet.m_DirectionY = -1
    
    var angle = point_direction(x, y, x + m_HorizDirection, y - 1);
    bullet.image_angle = angle;
    
    //we don't use our mouse anymore for shooting
    //var angle = point_direction(x, y, mouse_x, mouse_y);
    //bullet.m_DirectionX = cos(angle * (pi / 180));
    //bullet.m_DirectionY = sin(angle * (pi / 180));
    //bullet.image_angle = angle;
    
    m_ReloadTime = m_MaxReloadTime;
    m_NumRopes -= 1;
}
</string>
