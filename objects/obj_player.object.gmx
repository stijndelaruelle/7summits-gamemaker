        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Handle input
m_KeyLeft = false;
m_KeyRight = false;
m_KeyUp = false;
m_KeyDown = false;
m_KeyAction = false;
m_KeyJump = false;
var keyJump = false;
var keyJumpReleased = true;
var shootKey = false;

if (m_IsInputEnabled)
{
    m_KeyLeft = keyboard_check(vk_left) || keyboard_check(ord("A")) || keyboard_check(ord("Q"));
    m_KeyRight = keyboard_check(vk_right) || keyboard_check(ord("D"));
    m_KeyUp = keyboard_check(vk_up) || keyboard_check(ord("W")) || keyboard_check(ord("Z"));
    m_KeyDown = keyboard_check(vk_down) || keyboard_check(ord("S"));

    m_KeyJump = keyboard_check(vk_space);
    keyJump = keyboard_check_pressed(vk_space);
    keyJumpReleased = keyboard_check_released(vk_space);
    m_KeyAction = keyboard_check(vk_enter);
    shootKey = keyboard_check(vk_control);
}

//Check for ground
var onGround = place_meeting(x, y+1, par_solid);

if (onGround || m_IsOnLadder)
{
    m_VertSpeed = 0;
    m_VertDirection = 1;
    m_AirJumps = m_MaxAirJumps;
    
    //Jumping
    if (m_KeyJump &amp;&amp; !m_KeyDown)
    {
        m_VertSpeed = m_JumpSpeed; //Just so we leave the ground
        m_VertDirection = -1;
        m_IsOnLadder = false;
        m_IsJumping = true;
    }
}

//We're in the air
else
{
    m_VertSpeed += m_VertDirection * (global.Gravity * global.GameDeltaTime);
    
    //Jump height
    if (keyJumpReleased || m_VertDirection &gt; 0) { m_IsJumping = false; }
    
    if (m_IsJumping)
    {
        m_VertSpeed += 6.0 * global.GameDeltaTime;
        
        if (m_VertSpeed &gt;= m_MaxJumpSpeed)
        {
            m_IsJumping = false;
            m_VertSpeed = m_MaxJumpSpeed;
        }
        
        //var newVertSpeed = m_VertSpeed + (m_MaxJumpSpeed - m_VertSpeed) * 0.75;
        //if ((m_VertSpeed + global.Gravity) &gt;= newVertSpeed)
        //{
         //   m_IsJumping = false;
        //}
        //else
        //{
        //    m_VertSpeed = newVertSpeed;
        //}
    }

    //Airjumps
    if (m_AirJumps &gt; 0)
    {
        if (m_KeyJump &amp;&amp; !m_KeyDown)
        {
            m_VertSpeed = m_JumpSpeed;
            m_VertDirection = -1;
            m_IsJumping = true;
            --m_AirJumps;
        }
    }
    
    //Turn around mid air
    if (m_VertSpeed &lt;= 0)
    {
        m_VertDirection = 1; //Flip direction
        m_VertSpeed = 0;
    }
}

//Move left
if (m_KeyLeft)
{
    m_HorizSpeed = m_Speed;
    m_HorizDirection = -1;
}

//Move right
if (m_KeyRight)
{
    m_HorizSpeed = m_Speed;
    m_HorizDirection = 1;
}

//Ladder
if (!m_IsOnLadder)
{
    if (m_KeyUp || m_KeyDown)
    {
        if (place_meeting(x, y, par_ladder))
        {
            m_IsOnLadder = true;
        }
    }
}

if (m_IsOnLadder)
{
    if (m_KeyUp)   
    {
        m_VertDirection = -1;
        m_VertSpeed = (m_Speed * 2) / 3;
    }
    if (m_KeyDown)
    {
        m_VertDirection = 1;
        m_VertSpeed = (m_Speed * 2) / 3;
    }
    
    if (!place_meeting(x,y,par_ladder)) m_IsOnLadder = false;    
}

//Ducking
if (onGround &amp;&amp; !m_IsOnLadder)
{
    if (m_KeyDown)
    {
        m_IsDucking = true;
    }
}

if ((m_IsDucking &amp;&amp; !m_KeyDown) || !onGround)
{
    //Check if we can stand there
    mask_index = spr_player_mask;
    if (!place_meeting(x, y, par_solid))
    {
        m_IsDucking = false;
    }
    else
    {
        mask_index = spr_player_mask_crouch;
    }
}

//Not moving
if ((m_KeyLeft &amp;&amp; m_KeyRight) || (!m_KeyLeft &amp;&amp; !m_KeyRight))
{
    if (m_IsInputEnabled)
    {
        m_HorizSpeed = 0;
    }
}
</string>
